<script>  
function mobileNumber(){
	var Number = document.getElementById('ad_mobile_no').value;			 
	var IndNum = /^[0]?[6789]\d{9}$/;
	if(!IndNum.test(Number) && Number != ""){
		$('.mobile_number_check_error_1').show();
		$('.mobile_number_check_error_1').text("Invalid Mobile Number").show();
		return false;
	}			 
	else{		
		$('.mobile_number_check_error_1').hide();
		$('.mobile_number_check_error_1').text("");
		return true;
	}
}
</script>

 <script type="text/javascript">   
   function countRemainingCharacters(idName){
		var left = 500 - $('#'+idName).val().length;
		if (left < 0){
			left = 0;
		}
		$('#'+idName).parent('.AddressDiv').siblings('.remaingCharacterDiv').children('.RemaingCharSpan').text('( Characters left: ' + left+" )").css('font-size','10px');
		$('#'+idName).siblings('.RemaingCharSpan').text('Characters left: ' + left);
	}
		
	function calculateAge(id){
		var dob = $('#'+id).val();
		var splitDate = dob.split('/');
		dob = splitDate[1]+'/'+splitDate[0]+'/'+splitDate[2];
		if(dob){
			dob = new Date(dob);
			var today = new Date();
			var age = Math.floor((today-dob) / (365.25 * 24 * 60 * 60 * 1000));
			if(id=='dob'){
				var className = 'age_error_essage';
			}
			else if(id=='let_dob'){
				var className = 'age_error_essage_let';
			}
			
			if(age<18){
				$('.'+className).text('Age should be greater than or equal to 18').show();
			}
			else{
				$('.'+className).text('').hide();
			}
		}
	}
</script>
<script>
$(document).ready(function(){
	$(".datepickercalender").datepicker({
		format: 'dd/mm/yyyy',
		startDate: '01/01/1900',
		endDate: 'today',
		autoclose: true
	});

	$('#ad_enroll_no').keypress(function (e) {
		var regex = new RegExp("^[a-zA-Z0-9()-]+$");
		var str = String.fromCharCode(!e.charCode ? e.which : e.charCode);
		if (regex.test(str)){
			return true;
		}
		e.preventDefault();
		return false;
	});
	
	$('#ad_enroll_no').blur(function(e){
		validateEnrollmentNumber($(this).val());
	});
	$('#ad_enroll_no').keyup(function(e){
		validateEnrollmentNumber($(this).val());
	});
});
function validateEnrollmentNumber(str){
	if(str!=""){
		if(str.match(/^(?=.*\d)(?=.*[a-zA-Z()-])[a-zA-Z]{1}.{2,16}$/)){
			$('.enroll_number_check_error').text('').hide();
		}
		else{
			$('.enroll_number_check_error').text('Only Numbers and Only Special Characters Not Allowed').show();
		}
	}
}
</script>
<script type="text/javascript">
function getStates(){
	$('#adv_state').html("<option value=''> Select State</option>");
	var csrf = $('#csrftoken').val();
	$.ajax({
		type:"POST",
		cache:false,
		url:"ajax-responce.php",
		dataType:"JSON",
		data:'csrftoken='+csrf+'&QueryType=StateList',  
		success: function (data) {
			var csrf 	= data[0]['csrf'];
			var status 	= data[0]['status'];
			$('#csrftoken').val(csrf);
			
			if(status == 'OK' && data.length > 1){
				for(var i=1; i<data.length; i++){
					$('#adv_state').append("<option value='"+data[i]['id']+"'>"+data[i]['state_name']+"</option>");
				}
				$('#adv_state').val('28');
				setTimeout(function(){
					getDistrict();
				}, 500);
			}
			else if(status == 'CSRF'){
				$('.all_error_message_lawyer_form').text('Authentication failed. Please refresh page again.').show();
			}
			else{
			}
		}
	});
}

function getDistrict(){
	var stateVal=$('#adv_state').val();
	$('#adv_district').html("<option value=''>Select District</option>");
	if(stateVal != ""){
		var csrf = $('#csrftoken').val();
		$('.loading').show();
		$.ajax({
			type:"POST",
			cache:false,
			url:"ajax-responce.php",
			dataType:"JSON",
			data:'stateval='+btoa(stateVal)+'&csrftoken='+csrf+'&QueryType=DistrictList',  
			success: function (data) {
				var csrf   = data[0]['csrf'];
				var status = data[0]['status'];
				$('#csrftoken').val(csrf);
				
				if(status == 'OK' && data.length > 1){
					for(var i=1; i<data.length; i++){
						$('#adv_district').append("<option value='"+data[i]['id']+"'>"+data[i]['district_name']+"</option>");
					}
				}
				else if(status == 'CSRF'){
					$('.all_error_message_lawyer_form').text('Authentication failed. Please try again.').show();
				}
				$('.loading').hide();
			}
		});
	}
}
