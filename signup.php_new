			$('.alertMessage').html('Please check Invalid Mobile Number.');
			$('.showallerrormsg').modal('show');
			return false;
		}
		else{
			var mobile = '';
			if($('.passtype:checked').val() == 3){
				mobile = $.trim($('#ad_mobile_no').val());
			}
			else {
				mobile = $.trim($('#hdn_mobile').val());
			}
			var csrf = $.trim($('#csrftoken').val());
			$('.form_blank_error_message').hide();
			$('.loading').show();
			$.ajax({
				type:"POST",
				cache:false,
				url:"ajax-responce.php",
				data:"mobile="+btoa(mobile)+"&csrftoken="+csrf+"&QueryType=sendVarCode",
				success: function (html) {
					var dataArr = $.trim(html).split('##');
					$('#csrftoken').val(dataArr[1]);
					
					if(dataArr[0] == 'Yes'){
						$('.all_error_message_lawyer_form').text('').hide();
						timer2 = "02:59";
						var interval = setInterval(function() {
							var timer = timer2.split(':');

							//by parsing integer, I avoid all extra string processing
							var minutes = parseInt(timer[0], 10);
							var seconds = parseInt(timer[1], 10);
							--seconds;
							minutes = (seconds < 0) ? --minutes : minutes;
							if (minutes < 0) clearInterval(interval);
							seconds = (seconds < 0) ? 59 : seconds;
							seconds = (seconds < 10) ? '0' + seconds : seconds;

							if(parseInt(minutes) < 0 || (parseInt(minutes) == 0 && parseInt(seconds) == 0)){
								$('.btnresendotp').show();
								$('.countdown').html('Previous OTP has expired. Please resend.');
							}
							else{
								$('.countdown').html('OTP valid upto: '+minutes + ':' + seconds);
							}
							timer2 = minutes + ':' + seconds;
						}, 1000);
						
						$('#hiddenUserId').val($.trim($('ad_enroll_no').val()));
						
						$("#verification_code_modal").modal({
							backdrop: 'static',
							keyboard: false
						});
					}
					else if(dataArr[0] == 'CSRF'){
						$('.verification_code_error_text').text("Authentication failed.").show();
					}
					else{
						$('.verification_code_error_text').text("Incorrect Verification Code").show();
					}
					$('.loading').hide();
				}
			});
		}
	}
	
