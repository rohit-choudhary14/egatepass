	checkPwd1 = function () {
		var regularExpression  = /^(?=.*[0-9])(?=.*[a-z])(?=.*[A-Z])(?=.*[!@#$%^&*])[a-zA-Z0-9!@#$%^&*]{8,16}$/;
		var str = document.getElementById('ad_pass').value;
		if(str!="" && str!=null){
			$('.password_check_error_msg_for_lawyer').show();
			if (str.length < 8) {
			 
				$('.password_check_error_msg_for_lawyer').text("Password length should be atleast 8 characters");
				return ("too_short");
			}
			else if (str.length > 16) {
			   $('.password_check_error_msg_for_lawyer').text("Password length should be less than or equal to 16 characters");
				return ("too_long");
			} 
			else if(!regularExpression.test(str)) {
			 $('.password_check_error_msg_for_lawyer').text("password should contain atleast one Upper Case Letter, one Lower Case Letter, one Number and one Special character");
				return ("bad_char");
			}
		}
		$('.password_check_error_msg_for_lawyer').text("");
		return ("ok");
	}
});

